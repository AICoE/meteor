{
  "name": "meteor",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@kubernetes/client-node": "^0.15.0",
    "@patternfly/patternfly": "^4.125.3",
    "@patternfly/react-core": "^4.147.0",
    "@patternfly/react-log-viewer": "^4.4.0",
    "next": "^11.0.0",
    "next-transpile-modules": "^8.0.0",
    "prom-client": "^13.2.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "swr": "^0.5.6",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.16",
    "@types/react-dom": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.1",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "prettier": "^2.3.1",
    "typescript": "^4.3.4"
  },
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "build": "next build",
    "lint": "next lint && eslint src/**/*"
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": [
      "prettier"
    ],
    "extends": [
      "eslint:recommended",
      "prettier",
      "plugin:prettier/recommended",
      "plugin:react/recommended"
    ],
    "env": {
      "browser": true
    },
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx"
          ]
        }
      },
      "react": {
        "version": "detect"
      }
    }
  },
  "prettier": {
    "printWidth": 150,
    "arrowParens": "always",
    "semi": true,
    "tabWidth": 2,
    "singleQuote": true,
    "jsxSingleQuote": false,
    "bracketSpacing": true
  },
  "browserslist": {
    "production": [
      ">1%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
